@using System.Threading.Tasks
@model Admin.UI.CP.Pickup.Models.PickupListModel

<div class="row">
    <div class="col-xs-12">
        <h3 class="header smaller lighter blue">Shipments</h3>

        <div class="clearfix">
            <div>
                <a class="btn btn-success btn-add-shipment width-10" href="/shipment/0">
                    <i class="ace-icon fa fa fa-truck fa"></i>New Shipment
                </a>
            </div>
            <div class="pull-right tableTools-container"></div>

        </div>
        <div class="table-header">
            Shipment List
        </div>

        <!-- div.table-responsive -->
        <!-- div.dataTables_borderWrap -->
        <div>
            <table id="dynamic-table" class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Tracking #</th>
                        <th>Account No</th>
                        <th>Sender</th>
                        <th>Recipient</th>
                        <th>Shipment Date</th>
                        <th>Parcels</th>
                        <th>Weight</th>
                        <th>Service</th>
                        <th>Status</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>
<script src="~/lib/datatables/media/js/jquery.datatables.js"></script>
<script src="~/lib/datatables/media/js/datatables.bootstrap4.min.js"></script>
<script src="~/lib/datatables-tabletools/js/datatables.tabletools.js"></script>
<script src="~/assets/js/shipmentlist.js"></script>