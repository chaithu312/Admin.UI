@{

    Layout = "~/Views/shared/_layout.cshtml";
}
<div class="col-sm-7">
    <div class="widget-box">
        <div class="widget-header">
            <h4 class="widget-title">Pickup Request Detail</h4>
        </div>
        <div class="widget-body">
            <div class="widget-main">
                <form class="form-inline">
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Account No
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-9">
                            <input type="text" id="form-field-mask-1" class="form-control">
                            <span class="input-group-btn">
                                <button type="button" class="btn btn-sm btn-default">
                                    <i class="ace-icon fa fa-arrow-circle-right bigger-110"></i>
                                </button>
                            </span>
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Contact
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <select id="form-field-select-1" class="form-control">
                                <option value=""></option>
                            </select>
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Contact Name
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <input type="text" class="form-control" placeholder="" id="">
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Phone
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <input type="text" class="form-control" placeholder="" id="">
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Address
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <select id="form-field-select-1" class="form-control">
                                <option value=""></option>
                            </select>
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Address
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <input type="text" class="form-control" placeholder="" id=""><br />
                            &nbsp;
                            <input type="text" class="form-control" placeholder="" id="">
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            City
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <input type="text" class="form-control" placeholder="" id="">
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            State
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <select id="form-field-select-1" class="form-control">
                                <option value=""></option>
                            </select>
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Zip Code
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <input type="text" class="form-control" placeholder="" id="">
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Pickup From
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <input type="text" class="form-control" placeholder="" id="">
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Package Properties
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <input type="checkbox" style="float:left;"> <span style="padding-left:4px; padding-right:10px; float:left;">Document &nbsp; Estimated Weight:</span> <input type="text" class="form-control" style="display:inline-block; width:auto;">
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Total Pieces
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <select id="form-field-select-1" class="form-control">
                                <option value=""></option>
                            </select>
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="id-date-picker-1" class="col-sm-3">Date Picker</label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <!-- #section:plugins/date-time.datepicker -->
                            <div class="input-group">
                                <input type="text" data-date-format="dd-mm-yyyy" id="id-date-picker-1" class="form-control date-picker">
                                <span class="input-group-addon">
                                    <i class="fa fa-calendar bigger-110"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="timepicker1" class="col-sm-3">Time Picker</label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <div class="input-group bootstrap-timepicker">
                                <input type="text" class="form-control" id="timepicker1">
                                <span class="input-group-addon">
                                    <i class="fa fa-clock-o bigger-110"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="timepicker2" class="col-sm-3">Last Available Time</label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <div class="input-group bootstrap-timepicker">
                                <input type="text" class="form-control" id="timepicker2">
                                <span class="input-group-addon">
                                    <i class="fa fa-clock-o bigger-110"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Destination
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <select id="form-field-select-1" class="form-control">
                                <option value=""></option>
                            </select>
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Pickup Agent
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <select id="form-field-select-1" class="form-control">
                                <option value=""></option>
                            </select>
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Pickup Type
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <select id="form-field-select-1" class="form-control">
                                <option value=""></option>
                            </select>
                        </div>
                    </div>
                    <hr />
                    <div>
                        <label for="form-field-mask-1" class="col-sm-3">
                            Notes
                        </label>
                        <!-- #section:plugins/input.masked-input -->
                        <div class="input-group col-sm-8">
                            <textarea placeholder="Default Text" id="form-field-8" class="form-control"></textarea>
                        </div>
                    </div>
                    <div class="clearfix form-actions" style="margin-bottom:0px;">
                        <div class="text-right">
                            <button type="button" class="btn btn-info">
                                <i class="ace-icon fa fa-check bigger-110"></i>
                                Save
                            </button>
                            &nbsp; &nbsp; &nbsp;
                            <button type="reset" class="btn">
                                <i class="ace-icon fa fa-undo bigger-110"></i>
                                Save & New
                            </button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>