                                        <tr>

                                            <th>Weight</th>
                                            <th>Length</th>
                                            <th>Width</th>
                                            <th>Height</th>
                                        </tr>
                                        <tr ng:repeat="item in Parcel.items">
                                            <td><input type="number" min="0" ng:model="item.Weight" required class="form-control col-md-1"></td>
                                            <td><input type="number" min="0" ng:model="item.Length" required class="form-control col-md-1"></td>
                                            <td><input type="number" min="0" ng:model="item.Width" required class="form-control col-md-1"></td>
                                            <td><input type="number" min="0" ng:model="item.Height" required class="form-control col-md-1"></td>
                                            <td>

                                                <button type="button" class="btn btn-info btn-xs" ng-click="removeItem($index)">Remove Packet</button>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="5"><button type="button" class="btn btn-info btn-xs" ng-click="addItem()">Add Packet</button></td>
                                        </tr>
                                    </table>
