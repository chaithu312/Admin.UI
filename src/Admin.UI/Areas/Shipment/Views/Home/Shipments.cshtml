                                        <span class="input-group-addon">
                                            <i class="fa fa-calendar bigger-110"></i>
                                        </span>
                                    </div>
                                    <i ng-validation-icon="Shipments.$errors.shipmentdate"><label></label></i>
                                </div>*@

