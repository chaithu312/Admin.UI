@{
    Layout = "~/Views/shared/_layout.cshtml";
}

<div class="row">
    <div class="col-xs-12">
        <!-- PAGE CONTENT BEGINS -->
        <div class="center">

            <form name="ViewSupplyMaterial" data-ng-controller="ViewSupplyMaterialController" novalidate>

                <div class="pull-right tableTools-container">
                    <a class="btn-new-mail" href="/Shipment/SupplyMaterial">
                        <span class="btn btn-purple no-border">
                            <i class="ace-icon fa  bigger-130"></i>
                            <span class="bigger-110">Add Supply Material</span>
                        </span>
                    </a>
                </div>
                
                <div class="clearfix">
                    <div class="pull-right tableTools-container"></div>
                </div>

                <div>
                    <table my-table="overrideOptions" aa-data="datasrc" ao-column-defs="columnDefs" fn-row-callback="myCallback" class="table table-striped table-bordered table-hover dataTables_info">
                        <thead>
                            <tr>

                                <th style="width:200px">Name</th>
                                <th style="width:100px">Sequence</th>
                                <th style="width:100px">Unit</th>
                              
                                <th style="width:100px">Details</th>
                                
                            </tr>
                        </thead>
                    </table>
                </div>
            </form>
            <div id="errorMessages" class="error">{{message}}</div>
        </div>
    </div>
</div>
<link type="text/css" href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.12/themes/redmond/jquery-ui.css" rel="stylesheet" />
<script type="text/javascript" src="~/assets/js/jquery.min.js"></script>
<!-- DataTables -->
<script type="text/javascript" charset="utf8" src="http://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.8.2/jquery.dataTables.min.js">
</script>

<script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/buttons/1.1.0/js/dataTables.buttons.min.js">
</script>