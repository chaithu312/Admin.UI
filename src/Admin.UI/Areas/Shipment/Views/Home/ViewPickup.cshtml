@{
    Layout = "~/Views/shared/_layout.cshtml";
}

<div class="row">
    <div class="col-xs-12">
        <!-- PAGE CONTENT BEGINS -->
        <div class="center">

            <form name="ViewPickup" data-ng-controller="ViewPickupController" novalidate>
                <div id="errorMessages" class="error">{{message}}</div>
                <div class="clearfix">
                    <div class="pull-right tableTools-container"></div>
                </div>
                <div class="table-header">
                    List of all pickup
                </div>
                <div>
                    <table my-table="overrideOptions" aa-data="datasrc" ao-column-defs="columnDefs" fn-row-callback="myCallback" class="table table-striped table-bordered table-hover dataTables_info">
                        <thead>
                            <tr>

                                <th style="width:250px">Name</th>
                                <th style="width:100px">Phone</th>
                                <th style="width:100px">Address</th>
                                <th style="width:100px">City</th>

                                <th style="width:100px">PickupFrom</th>

                                <th style="width:100px">Ready By</th>
                                <th style="width:100px">Available Until</th>

                                <th style="width:100px">Total #</th>
                                <th style="width:100px">Destination</th>
                                <th style="width:100px">Instructions</th>
                                <th style="width:100px">Details</th>
                                <th style="width:100px">Confirmation</th>
                                <th style="width:100px"></th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </form>
        </div>
    </div>
</div>

<script type="text/javascript" src="~/assets/js/jquery.min.js"></script>
<!-- DataTables -->
<script type="text/javascript" charset="utf8" src="http://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.8.2/jquery.dataTables.min.js">
</script>

<script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/buttons/1.1.0/js/dataTables.buttons.min.js">
</script>